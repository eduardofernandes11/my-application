<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_main"
        tools:context=".ui.MainActivity">
    <LinearLayout
            android:id="@+id/outterLinearLayout"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <LinearLayout
                android:layout_marginTop="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="270dp">

            <android.support.v7.widget.CardView
                    android:id="@+id/cardView0"
                    android:layout_width="190dp"
                    android:layout_height="250dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:visibility="visible"
                    app:cardCornerRadius="6dp"
                    app:cardElevation="6dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                <android.support.constraint.ConstraintLayout
                        android:id="@+id/linearLayout0"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="6dp">

                    <LinearLayout
                            android:id="@+id/linearLayout5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="parent"
                            tools:ignore="MissingConstraints">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_bold"
                                android:text="Maça Fuji"
                                android:textSize="25sp" />

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="8dp"
                                android:fontFamily="@font/roboto_light"
                                android:text="0,99€ por Kg"
                                android:textSize="20sp" />

                    </LinearLayout>

                    <ImageButton
                            android:id="@+id/apple"
                            android:layout_width="137dp"
                            android:layout_height="165dp"
                            android:backgroundTint="#FFFFFF"
                            android:scaleType="centerInside"
                            android:tag="0"
                            app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/apple"
                            tools:ignore="MissingConstraints" />

                </android.support.constraint.ConstraintLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                    android:id="@+id/cardView1"
                    android:layout_width="190dp"
                    android:layout_height="250dp"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:cardCornerRadius="6dp"
                    app:cardElevation="6dp">
                <android.support.constraint.ConstraintLayout
                        android:id="@+id/linearLayout1"
                        android:layout_margin="6dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    <ImageButton
                            android:id="@+id/alface"
                            android:layout_width="127dp"
                            android:layout_height="146dp"
                            android:background="#FFFFFF"
                            android:scaleType="centerInside"
                            android:tag="1"
                            app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.509"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/alface"
                            tools:ignore="MissingConstraints" />

                    <LinearLayout
                            android:id="@+id/linearLayout4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="parent"
                            tools:ignore="MissingConstraints">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_bold"
                                android:text="Alface Roxa"
                                android:textSize="25sp" />

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="8dp"
                                android:fontFamily="@font/roboto_light"
                                android:text="1,29€ por Kg"
                                android:textSize="20sp" />

                    </LinearLayout>

                </android.support.constraint.ConstraintLayout>
            </android.support.v7.widget.CardView>

        </LinearLayout>

        <LinearLayout
                android:layout_marginTop="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="270dp">
            <android.support.v7.widget.CardView
                    android:id="@+id/cardView2"
                    android:layout_width="190dp"
                    android:layout_height="250dp"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:cardCornerRadius="6dp"
                    app:cardElevation="6dp">
                <android.support.constraint.ConstraintLayout
                        android:id="@+id/linearLayout2"
                        android:layout_margin="6dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    <ImageButton
                            android:id="@+id/batata"
                            android:layout_width="127dp"
                            android:layout_height="146dp"
                            android:background="#FFFFFF"
                            android:scaleType="centerInside"
                            android:tag="2"
                            app:layout_constraintBottom_toTopOf="@+id/linearLayout10"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.509"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/batata"
                            tools:ignore="MissingConstraints" />

                    <LinearLayout
                            android:id="@+id/linearLayout10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="parent"
                            tools:ignore="MissingConstraints">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_bold"
                                android:text="Batata"
                                android:textSize="25sp" />

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="8dp"
                                android:fontFamily="@font/roboto_light"
                                android:text="0,79€ por Kg"
                                android:textSize="20sp" />

                    </LinearLayout>
                </android.support.constraint.ConstraintLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                    android:id="@+id/cardView3"
                    android:layout_width="190dp"
                    android:layout_height="250dp"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:cardCornerRadius="6dp"
                    app:cardElevation="6dp">

                <android.support.constraint.ConstraintLayout
                        android:id="@+id/linearLayout3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="6dp">

                    <ImageButton
                            android:id="@+id/bete"
                            android:layout_width="127dp"
                            android:layout_height="146dp"
                            android:background="#FFFFFF"
                            android:scaleType="centerCrop"
                            android:tag="3"
                            app:layout_constraintBottom_toTopOf="@+id/linearLayout11"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.509"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/beterraba"
                            tools:ignore="MissingConstraints" />

                    <LinearLayout
                            android:id="@+id/linearLayout11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="parent"
                            tools:ignore="MissingConstraints">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_bold"
                                android:text="Beterraba"
                                android:textSize="25sp" />

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="8dp"
                                android:fontFamily="@font/roboto_light"
                                android:text="1,59€ por Kg"
                                android:textSize="20sp" />

                    </LinearLayout>
                </android.support.constraint.ConstraintLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>

        <LinearLayout
                android:layout_marginTop="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="270dp">
            <android.support.v7.widget.CardView
                    android:id="@+id/cardView4"
                    android:layout_width="190dp"
                    android:layout_height="250dp"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:cardCornerRadius="6dp"
                    app:cardElevation="6dp">
                <android.support.constraint.ConstraintLayout
                        android:id="@+id/linearLayout6"
                        android:layout_margin="6dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    <LinearLayout
                            android:id="@+id/linearLayout7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="parent"
                            tools:ignore="MissingConstraints">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_bold"
                                android:text="Cenoura"
                                android:textSize="25sp" />

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="8dp"
                                android:fontFamily="@font/roboto_light"
                                android:text="0,75€ por Kg"
                                android:textSize="20sp" />

                    </LinearLayout>

                    <ImageButton
                            android:id="@+id/cenoura"
                            android:layout_width="137dp"
                            android:layout_height="165dp"
                            android:backgroundTint="#FFFFFF"
                            android:scaleType="centerInside"
                            app:layout_constraintBottom_toTopOf="@+id/linearLayout7"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/cenoura1"
                            tools:ignore="MissingConstraints" />

                </android.support.constraint.ConstraintLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                    android:id="@+id/cardView5"
                    android:layout_width="190dp"
                    android:layout_height="250dp"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:cardCornerRadius="6dp"
                    app:cardElevation="6dp">
                <android.support.constraint.ConstraintLayout
                        android:id="@+id/linearLayout8"
                        android:layout_margin="6dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    <ImageButton
                            android:id="@+id/morango"
                            android:layout_width="127dp"
                            android:layout_height="146dp"
                            android:background="#FFFFFF"
                            android:scaleType="centerInside"
                            app:layout_constraintBottom_toTopOf="@+id/linearLayout9"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.509"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/morango"
                            tools:ignore="MissingConstraints" />

                    <LinearLayout
                            android:id="@+id/linearLayout9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="parent"
                            tools:ignore="MissingConstraints">

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_bold"
                                android:text="Morango"
                                android:textSize="25sp" />

                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="8dp"
                                android:fontFamily="@font/roboto_light"
                                android:text="3,38€ por Kg"
                                android:textSize="20sp" />

                    </LinearLayout>

                </android.support.constraint.ConstraintLayout>
            </android.support.v7.widget.CardView>

        </LinearLayout>

    </LinearLayout>
</ScrollView>